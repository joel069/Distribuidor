<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">

	<navigation-rule>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>exito</from-outcome>
			<to-view-id>/privada/VistaAdministrador.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fallo</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>exitoEmple</from-outcome>
			<to-view-id>/privada/VistaEmpleado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/privada/VistaAdministrador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>producto</from-outcome>
			<to-view-id>/ProductoJSF.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/privada/VistaAdministrador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>distribuidora</from-outcome>
			<to-view-id>/resource/Distribuidora.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/privada/VistaAdministrador.xhtml</from-view-id>
		<navigation-case>
		<from-outcome>crearFactura</from-outcome>
			<to-view-id>/privada/CrearFactura.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/privada/VistaAdministrador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registroUsuario</from-outcome>
			<to-view-id>/privada/RegistroUsuarios.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/privada/VistaAdministrador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rol</from-outcome>
			<to-view-id>/privada/RegistroRol.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>







</faces-config>
